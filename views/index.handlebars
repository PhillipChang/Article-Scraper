    <h1>Article Scraper <button class="btn" href="/scrape">Scrape Most Recent Articles</button></h1>
      <div id="articles">
                 {{#each article}}
                    {{> articles-block}}
                 {{/each}}
                </div>


    <div id="myModal" class="modal">
        <div class="modal-content">
            </div>
            <div class="modal-footer">
                <a href="#!" id="save" class="modal-close waves-effect waves-green btn-flat" value="1">Save Note</a>
                <a href="#!" id="no" class="modal-close waves-effect waves-green btn-flat" value="0">Cancel</a>
            </div>
    </div>