    <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{title}}</span>
          <p>{{link}}</p>
        </div>
        <div class="card-action">
          <button class="btn modal-trigger"  data-target="myModal" href="#myModal" id="add-note" data-id="{{_id}}" data-title="{{title}}" data-link="{{link}}">Add Note</button>
          <a class="change-save modal-trigger" href="#" id="saveNote" data-id="{{_id}}" data-title="{{title}}" data-link="{{link}}">Save Note</a>
        </div>
      </div>
    </div>
    </div>


 

