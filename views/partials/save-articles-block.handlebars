    <div class="row">
    <div class="col s12 m12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{title}}</span>
          <a id="link" href="{{link}}">{{link}}</a>
        </div>
        <div class="card-action">
          <button class="btn modal-trigger"  data-target="myModal" href="#myModal" id="add-note" data-id="{{_id}}" data-title="{{title}}" data-link="{{link}}">Add Note</button>
          <button class="btn modal-trigger"  data-target="noteModal" href="#noteModal" id="view-note" data-id="{{_id}}" data-title="{{title}}" data-link="{{link}}">View Notes</button>
        </div>
      </div>
    </div>
    </div>

        <div id="myModal" class="modal">
        <div class="modal-content" id="modal-content-add">
            <h6> Would you like to add a Note to: {{title}}</h6>
            <input id="titleinput" type="text" placeholder="Enter title of note"> 
            <input id="bodyinput" type="text" placeholder= "Enter notes">
            </div>
            <div class="modal-footer"> 
                <button class="btn" href="#" id="save" data-id="{{_id}}" data-title="{{title}}" data-link="{{link}}">Save Note</button>
                <a href="#!" id="no" class="modal-close waves-effect waves-green btn-flat" value="0">Cancel</a>
            </div>
    </div>
    <div id="noteModal" class="modal">
        <div class="modal-content" id="modal-content-view">
            <h6 id="note-title">{{title}}</h6>
            <p>{{note.body}}</p>
            </div>
            <div class="modal-footer">
                <a href="#!" class="modal-close waves-effect waves-green btn-flat" value="0">Close</a>
            </div>
    </div>