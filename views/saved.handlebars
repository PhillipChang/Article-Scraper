    <h1>Saved Articles</h1> <a href="/" <button class="btn">Home</button></a>
    <div id="articles">
                 {{#each article}}
                 {{#if saved}}
                    {{> save-articles-block}}
                 {{/if}}
                 {{/each}}
                </div>

    <div id="myModal" class="modal">
        <div class="modal-content" id="modal-content-add">
            </div>
            <div class="modal-footer">
                <a href="#!" id="save" class="modal-close waves-effect waves-green btn-flat" value="1">Save Note</a>
                <a href="#!" id="no" class="modal-close waves-effect waves-green btn-flat" value="0">Cancel</a>
            </div>
    </div>
    <div id="noteModal" class="modal">
        <div class="modal-content" id="modal-content-view">
            </div>
            <div class="modal-footer">
                <a href="#!" class="modal-close waves-effect waves-green btn-flat" value="0">Close</a>
            </div>
    </div>
